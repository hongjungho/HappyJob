//회원가입하기
this.Button02_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.confirm("저장하시겠습니까?")){
		if(this.Div01.Edit00.value == null){
			alert("이름을 기입해주세요.");
		}
		else if(this.Div01.Edit00.value != null && new RegExp( /^[가-힣]{2,10}/).test(this.Div01.Edit00.value)==false )
		{
			alert("이름은  공백 없이 한글 10글자까지만 입력해주세요.")
		}
		else if(this.Div01.Div02.Edit00.value == null){
			alert("아이디 중복 확인을 해주세요.");
		}
		else if(this.Div01.Div04.Edit00.value == null){
			alert("비밀번호를 기입해주세요.");
		}
		else if (this.Div01.Div04.Edit00.value != null && new RegExp(/^[a-z0-9.*\W]{5,10}/).test(this.Div01.Div04.Edit00.value)==false ){
			alert( "비밀번호는 공백없이 영어 소문자, 숫자, 특수기호로 이루어진 5~10자 이내여야 합니다.");
		}
		else if((this.Div01.Div04.Edit00.value) != (this.Div01.Div05.Edit00.value)){
			alert("비밀번호가 일치하지 않습니다.");
		}
		else if(this.Div01.Div01.Calendar00.value == null){
			alert("생년월일을 기입해주세요.");
		}
		else if(this.Div01.Div01.Combo00.value == null){
			alert("성별을 기입해주세요.");
		}
		else if(this.Div01.Div08.cmb_tel.value == null){
			alert("휴대폰번호을 기입해주세요.");
		}
		else if(this.Div03.Radio00.value == null ){
			alert("필수사항에 동의해주세요.");
		}
		else if(this.Div03.Radio00.value != null && this.Div03.Radio00.value != 1){ 
			alert("필수사항 동의시에 가입이 가능합니다.");
		}
		else{
		this.dsSchMap.setColumn(1,"userPhoneFul",this.Div01.Div08.cmb_tel1.text+"-"+this.Div01.Div08.Edit00.value+"-"+this.Div01.Div08.Edit01.value)
		this.dsSchMap.setColumn(1,"userEmailFul",this.Div01.Div09.Edit00.value+"@"+this.Div01.Div09.Edit01.value)
		this.gfn_serviceCall("saveUserListForm");
		alert("회원가입완료 로그인페이지로 이동합니다.");
		
		application.mainframe.VFrameSet0.VFrameSet1.HFrameSet0.ChildFrame2.set_visible(false);
		application.mainframe.VFrameSet0.VFrameSet1.HFrameSet0.ChildFrame3.set_left(0);	
		application.mainframe.VFrameSet0.VFrameSet1.HFrameSet0.ChildFrame3.set_width(1024);	
		var sURL = "Practice::login.xfdl";
		this.gfn_main(sURL);
		
		}
	}
}

