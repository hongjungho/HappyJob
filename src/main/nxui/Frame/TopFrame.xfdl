   this.stNm.set_text(Iject.$["usernm"] + " <b v='false'>님 환영합니다.</b>");
}


//도움말
this.btn_help_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.alert("도움말은 준비중 입니다.");
}
