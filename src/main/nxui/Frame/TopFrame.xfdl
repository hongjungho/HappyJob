include "Lib::Comm.xjs";

/*===============================================================================
* Form 관련 Script
*=============================================================================*/

//form onload
this.TopFrame_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	
}

/*===============================================================================
* Component 관련 Script
*=============================================================================*/

//좌측메뉴 펼침 접힙 실행
this.btn_leftMenuOpen_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
