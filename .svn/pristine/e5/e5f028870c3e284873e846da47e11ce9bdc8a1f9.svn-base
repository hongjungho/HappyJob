<?xml version="1.0" encoding="utf-8"?>
<FDL version="1.5">
  <TypeDefinition url="..\default_typedef.xml"/>
  <Form id="sidemenu" left="0" top="0" width="200" height="768" titletext="New Form" onload="fn_onload">
    <Layouts>
